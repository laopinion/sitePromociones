{
  "name": "promociones",
  "version": "2.0.0",
  "description": "Site dedicado a las promociones",
  "main": "server.js",
  "scripts": {
    "dev": "node server.js",
    "build": "next build",
    "start": "NODE_ENV=production node server.js",
    "startWind": "cross-env NODE_ENV=production node server.js",
    "lint": "standard --fix"
  },
  "standard": {
    "parser": "babel-eslint"
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "commonjs": true,
      "es6": true,
      "node": true,
      "jquery": true
    },
    "parser": "babel-eslint",
    "rules": {
      "no-console": "warn",
      "no-use-before-define": "warn",
      "spaced-comment": "warn"
    },
    "extends": [
      "standard",
      "standard-jsx"
    ]
  },
  "eslintIgnore": [
    "/node_modules/*",
    "build/*",
    "!build/index.js",
    "bundle.js",
    "dist/*",
    ".*/*",
    ".*"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/laopinion/sitePromociones.git"
  },
  "keywords": [
    "promociones"
  ],
  "author": "LaOpinion",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/laopinion/sitePromociones/issues"
  },
  "homepage": "https://github.com/laopinion/sitePromociones#readme",
  "dependencies": {
    "@zeit/next-sass": "^1.0.1",
    "body-parser": "1.19.0",
    "dotenv": "8.0.0",
    "express": "4.17.1",
    "next": "8.1.0",
    "node-sass": "^4.12.0",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "react-google-invisible-recaptcha": "0.2.11",
    "react-markdown": "4.0.8",
    "superagent": "5.0.5"
  },
  "devDependencies": {
    "babel-eslint": "10.0.1",
    "cross-env": "5.2.0",
    "eslint": "5.16.0",
    "eslint-config-standard": "12.0.0",
    "eslint-config-standard-jsx": "6.0.2",
    "standard": "12.0.1"
  }
}
